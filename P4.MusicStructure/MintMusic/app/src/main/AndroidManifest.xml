<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.cybersapien.mintmusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NowPlaying"
            android:label="@string/title_activity_now_playing" />
        <activity
            android:name=".SongDetails"
            android:label="Song Detail" />
        <activity android:name=".playlistDetail"
            android:label="@string/playList_activity_label"/>
        <activity android:name=".myaccount"
            android:label="@string/my_account_activityName"/>
    </application>

</manifest>